{
	"name": "cye-nextjs-frontend",
	"version": "0.1.0",
	"private": true,
	"scripts": {
		"dev": "cross-env NODE_OPTIONS='--inspect' next dev",
		"build": "next build",
		"production": "next build && next start",
		"lint": "next lint",
		"build-transform-colors": "npx token-transformer src/styles/tokens/colors.json src/styles/output/",
		"build-transform-text-styles": "npx token-transformer src/styles/tokens/text-styles.json src/styles/output/",
		"build-transform": "npm run build-transform-colors && npm run build-transform-text-styles",
		"build-tailwind": "node style-token.js",
		"build-styles": "npm run build-transform && node style-token.js"
	},
